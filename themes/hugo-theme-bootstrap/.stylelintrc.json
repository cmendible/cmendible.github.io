{
    "extends":"stylelint-config-standard",
    "plugins":[
        "stylelint-scss"
    ],
    "rules":{
        "at-rule-no-unknown":null,
        "scss/at-rule-no-unknown":true,
        "font-family-no-missing-generic-family-keyword":[
            true,
            {
                "ignoreFontFamilies":[
                    "Font Awesome 5 Free"
                ]
            }
        ]
    }
}
